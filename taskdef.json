{
    "executionRoleArn": "arn:aws:iam::962201869537:role/ecsTaskExecutionRole",
    "containerDefinitions": [
        {
            "name": "my-container",
            "image": "962201869537.dkr.ecr.ap-south-1.amazonaws.com/ecr-repo-new:latest",
            "essential": true,
            "portMappings": [
                {
                    "hostPort": 8080,
                    "protocol": "tcp",
                    "containerPort": 8080
                }
            ]
        }
    ],
    "requiresCompatibilities": [
        "EC2"
    ],
    "networkMode": "bridge",
    "cpu": "256",
    "memory": "512",
    "family": "ecs-node"
}
